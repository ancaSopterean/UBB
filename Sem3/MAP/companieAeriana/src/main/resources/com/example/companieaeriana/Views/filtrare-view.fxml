<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.companieaeriana.Controller.FiltrareController">
    <children>
        <TableView fx:id="flightsTable" prefHeight="250.0" prefWidth="600.0">
            <columns>
                <TableColumn fx:id="fromColumn" prefWidth="120.0" text="from" />
                <TableColumn fx:id="toColumn" prefWidth="120.0" text="to" />
                <TableColumn fx:id="departureTimeColumn" prefWidth="120.0" text="departureTime" />
                <TableColumn fx:id="landingTimeColumn" prefWidth="120.0" text="landingTime" />
            <TableColumn fx:id="seatsColumn" prefWidth="120.0" text="seats" />
            </columns>
        </TableView>
      <ComboBox fx:id="fromCBox" layoutX="26.0" layoutY="263.0" prefWidth="150.0" promptText="from" />
      <ComboBox fx:id="toCBox" layoutX="26.0" layoutY="303.0" prefWidth="150.0" promptText="to" />
      <DatePicker fx:id="datePicker" layoutX="13.0" layoutY="345.0" />
      <Button layoutX="380.0" layoutY="304.0" mnemonicParsing="false" onAction="#handleCumpara" text="cumpara" />
    </children>
</AnchorPane>
